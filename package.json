	"scripts": {
		"win:web": "set NODE_ENV=development & webpack-dev-server --open --mode development --config ./config/webpack.config.js --hot",
		"start:web": "NODE_ENV=development webpack-dev-server --open --mode development --config ./config/webpack.config.js --hot",
		"build:web": "npm run clean && NODE_ENV=production webpack --mode production --config ./config/webpack.prod.js",
		"build:web:win": "set NODE_ENV=production & webpack --mode production --config ./config/webpack.prod.js",
		"build:staging:web": "set NODE_ENV=staging & webpack --mode production --config ./config/webpack.prod.js",
		"build:production:web": "set NODE_ENV=production webpack --mode production --config ./config/webpack.prod.js",
		"prepare": "husky install",
		"lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
		"lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
		"prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
		"pre:commit": "npm run prettier && npm run lint:fix",
		"clean": "rm -rf build"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"**/*.{js,jsx,ts,tsx}": "npm run lint:fix",
		"**/*.{js,jsx,tsx,ts,less,md,json}": [
			"prettier --write"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
